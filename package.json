{
  "name": "phpvms-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "ci": "yarn lint && yarn format:diff",
    "lint": "eslint --cache \"**/*.js\" && stylelint \"**/*.css\"",
    "format": "prettier --config .prettierrc --write \"**/*.{js,jsx,ts,tsx,md,mdx}\"",
    "format:diff": "prettier --config .prettierrc --list-different \"**/*.{js,jsx,ts,tsx,md,mdx}\"",
    "dev": "docusaurus start"
  },
  "dependencies": {
    "@docusaurus/core": "~3",
    "@docusaurus/plugin-client-redirects": "~3",
    "@docusaurus/plugin-ideal-image": "~3",
    "@docusaurus/plugin-sitemap": "~3",
    "@docusaurus/preset-classic": "~3",
    "@docusaurus/theme-live-codeblock": "~3",
    "@mdx-js/react": "^3",
    "clsx": "^1.2.1",
    "file-loader": "^6.2.0",
    "nan": "^2.14.2",
    "prettier": "^3.5.3",
    "prism-react-renderer": "^2.1.0",
    "rehype-truncate": "^1.2.2",
    "remark-images": "^2.0.0",
    "url-loader": "^4.1.1"
  },
  "engines": {
    "node": ">=22"
  }
}
